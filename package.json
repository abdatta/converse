{
  "name": "converse",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -i \"npm:server -s\" \"npm:client -s\"",
    "client": "ng serve -o",
    "server": "cd server && npm start -s",
    "build": "concurrently \"ng build --prod\" \"cd server && npm run build\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
